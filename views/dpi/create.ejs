<%- include("../partials/head") %>
<%- include("../partials/sidebar") %>
<%- include("../partials/topbar") %>

    <div id="layoutSidenav_content">
        <main>
            <div class="container-fluid px-4">
                <h1 class="mt-4">Dashboard</h1>
                <ol class="breadcrumb mb-4">
                    <li class="breadcrumb-item active">Dashboard</li>
                </ol>
                <h3 class="mt-5">Tambah Data DPI</h3>
                <form method="post" action="/dpi/store" class="row g-3">
                    <div class="col-md-6">
                        <div class="form-group">
                            <label for="nama_dpi" class="form-label">Nama DPI</label>
                            <input type="text" class="form-control" id="nama_dpi" name="nama_dpi" required>
                        </div>
                        <div class="form-group">
                            <label for="luas" class="form-label">Luas</label>
                            <input type="number" class="form-control" id="luas" name="luas" required>
                        </div>
                    </div>
                    <div class="col-12">
                        <div class="text-end">
                            <button type="submit" class="btn btn-success">Simpan</button>
                        </div>
                    </div>
                </form>
            </div>
        </main>




        <%- include("../partials/footer") %>



